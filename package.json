{
  "name": "png-tuber",
  "version": "1.0.0",
  "description": "PNG Tuber software with customization website using Electron",
  "main": "dist/main/main.js",
  "scripts": {
    "start": "npm run build && electron .",
    "build": "npm run tsc && npx tailwindcss-cli@latest build -o src/styles/globals.css && npx webpack",
    "tsc": "tsc",
    "watch": "npx webpack --watch",
    "tailwind": "npx tailwindcss-cli@latest build -o src/styles/globals.css"
  },
  "dependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "electron": "^31.3.1",
    "html-webpack-plugin": "^5.6.0",
    "postcss": "^8.4.40",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "css-loader": "^6.8.1",
    "postcss-loader": "^7.3.2",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.1",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4"
  }
}
